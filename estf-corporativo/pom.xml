	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-javadoc-plugin</artifactId>
				<version>2.10.4</version>
			</plugin>
			<plugin>
				<artifactId>maven-antrun-plugin</artifactId>
				<executions>
					<execution>
						<id>Instrument domain classes</id>
						<configuration>
							<tasks>
								<taskdef name="instrument" classname="org.hibernate.tool.instrument.cglib.InstrumentTask">
									<classpath>
										<path refid="maven.dependency.classpath" />
										<path refid="maven.plugin.classpath" />
									</classpath>
								</taskdef>
								<instrument verbose="true">
									<fileset dir="${project.build.outputDirectory}">
										<include name="**/Agendamento.class" />
										<include name="**/Alerta.class" />
										<include name="**/AndamentoPeticao.class" />
										<include name="**/AndamentoProcesso.class" />
										<include name="**/Comunicacao.class" />
										<include name="**/ControleVoto.class" />
										<include name="**/DocumentoTexto.class" />
										<include name="**/DocumentoEletronicoView.class" />
										<include name="**/IncidenteAnalise.class" />
										<include name="**/IncidenteJulgamento.class" />
										<include name="**/ItemControle.class" />
										<include name="**/JulgamentoProcesso.class" />
										<include name="**/ObjetoIncidente.class" />
										<include name="**/PecaProcessoEletronico.class" />
										<include name="**/Peticao.class" />
										<include name="**/PeticaoEletronica.class" />
										<include name="**/PeticaoSetor.class" />
										<include name="**/Processo.class" />
										<include name="**/ProcessoImagem.class" />
										<include name="**/ProcessoIntegracao.class" />
										<include name="**/ProcessoPublicado.class" />
										<include name="**/ProcessoSetor.class" />
										<include name="**/ProcessoTema.class" />
										<include name="**/Protocolo.class" />
										<include name="**/RecursoProcesso.class" />
										<include name="**/ReferenciaPrescricao.class" />
										<include name="**/RotuloObjetoIncidente.class" />
										<include name="**/Sumula.class" />
										<include name="**/Texto.class" />
										<include name="**/VinculoProcessoTema.class" />
									</fileset>
								</instrument>
							</tasks>
						</configuration>
						<phase>process-classes</phase>
						<goals>
							<goal>run</goal>
						</goals>
					</execution>
				</executions>
				<dependencies>
					<dependency>
						<groupId>javax.mail</groupId>
						<artifactId>mail</artifactId>
						<version>1.5.0-b01</version>
					</dependency>
					<dependency>
						<groupId>javax.activation</groupId>
