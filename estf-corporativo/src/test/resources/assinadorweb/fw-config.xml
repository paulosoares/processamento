<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jboss-web PUBLIC "-//JBoss//DTD Web Application 2.2//EN" "http://www.jboss.org/j2ee/dtd/jboss-web.dtd">

<fw-config log="false">

	<cache disabled="true" />

	<max-query-result value="150" />

	<spring singleton-container="false">
		<bean-factory-key>beanFactorySTF</bean-factory-key>
	</spring>

	<security enabled="true" database="false">
		<application-name>ESTFASSINADOR</application-name>
	</security>

	<user>
		<anonymous>anonimoassinadorweb</anonymous>

		<class>br.gov.stf.estf.assinatura.security.UsuarioAssinatura</class>

		<event-listeners>
			<event-listener>br.gov.stf.estf.assinatura.security.UsuarioAssinaturaEventListener
			</event-listener>
		</event-listeners>

		<type value="UT_ACEGI_SECURITY" />
	</user>

</fw-config>