	<style name="AlinhadoDireita" style="Texto" hAlign="Right" fontName="Times New Roman" fontSize="12" isBold="false" isItalic="false" isUnderline="false" isStrikeThrough="false"/>
	<style name="RotuloCentralizado" style="Rotulo" hAlign="Center"/>
	<parameter name="imprimePostal" class="java.lang.String">
		<defaultValueExpression><![CDATA["N"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT g.num_guia as numeroDaGuia,
       g.ano_guia as anoDaGuia,
       g.cod_orgao_origem as codigoOrgaoOrigem,
       sorigem.dsc_setor as descricaoOrgaoOrigem,
       g.cod_orgao_destino as codigoOrgaoDestino,
       sdestino.dsc_setor as descricaoOrgaoDestino,
       pr.sig_classe_proces as siglaClasseProcesso,
       pr.num_processo as numeroProcesso,
       pr.qtd_volumes as quantidadeVolumes,
       pr.qtd_apensos as quantidadeApensos,
       pr.qtd_juntada_linha as quantidadeJuntadaLinha
  FROM stf.guias g,
       stf.desloca_processos dp,
       judiciario.processo pr,
       stf.setores sorigem,
       stf.setores sdestino
 WHERE     g.num_guia = dp.num_guia
       AND g.ano_guia = dp.ano_guia
       AND g.cod_orgao_origem = dp.cod_orgao_origem
       AND dp.num_processo = pr.num_processo
       AND dp.sig_classe_proces = pr.sig_classe_proces
       AND dp.cod_orgao_origem = sorigem.cod_setor
       AND dp.cod_orgao_destino = sdestino.cod_setor
       AND g.ano_guia = 2012
       AND g.num_guia = 7
       AND g.cod_orgao_origem = 600000151]]>
	</queryString>
	<field name="dataRemessa" class="java.util.Date">
		<fieldDescription><![CDATA[dataRemessa]]></fieldDescription>
	</field>
	<field name="descricaoOrgaoExterno" class="java.lang.String">
		<fieldDescription><![CDATA[descricaoOrgaoExterno]]></fieldDescription>
	</field>
	<field name="numeroProcesso" class="java.lang.Long">
		<fieldDescription><![CDATA[numeroProcesso]]></fieldDescription>
	</field>
	<field name="relator" class="java.lang.String">
		<fieldDescription><![CDATA[relator]]></fieldDescription>
	</field>
	<field name="siglaClasseProcesso" class="java.lang.String">
		<fieldDescription><![CDATA[siglaClasseProcesso]]></fieldDescription>
	</field>
	<field name="numeroOrgaoDestino" class="java.lang.String"/>
	<variable name="totalProcesso" class="java.lang.Long" calculation="Sum">
		<variableExpression><![CDATA[($F{numeroProcesso}!=null ? 1:1)]]></variableExpression>
		<initialValueExpression><![CDATA[0]]></initialValueExpression>
	</variable>
	<group name="OrgaoExterno">
		<groupExpression><![CDATA[$F{descricaoOrgaoExterno}]]></groupExpression>
		<groupHeader>
