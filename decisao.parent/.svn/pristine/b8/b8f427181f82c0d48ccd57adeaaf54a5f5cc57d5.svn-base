				<assinadorMobile.baseTestSrcDir>src/test/javascript/mobile/assinador</assinadorMobile.baseTestSrcDir>
				<assinadorMobile.externalTestSrcDir>src/test/javascript/mobile-external</assinadorMobile.externalTestSrcDir>
