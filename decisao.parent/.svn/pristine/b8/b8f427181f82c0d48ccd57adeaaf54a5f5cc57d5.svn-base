							</sourceIncludes>
							<sourceExcludes>
								<exclude>${assinadorMobile.relativeFromSrAssinadorDir}/assets_stf/js/app/app-bootstrap.js</exclude>
							</sourceExcludes>
							<jsTestSrcDir>${assinadorMobile.baseTestSrcDir}/specs</jsTestSrcDir>
							<preloadSources>
