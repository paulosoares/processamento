								<source>${assinadorMobile.baseTestSrcDir}/lib/jasmine-jquery.js</source>
								<source>${assinadorMobile.baseTestSrcDir}/lib/jasmine-console-fallback.js</source>
							</preloadSources>
						</configuration>
					</plugin>
				</plugins>
			</build>
			<properties>
				<jasmine.version>1.3.1.6</jasmine.version>
				<assinadorMobile.relativeFromSrAssinadorDir>main/webapp/mobile/assinador</assinadorMobile.relativeFromSrAssinadorDir>
