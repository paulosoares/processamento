		<profile>
			<id>publish-jboss</id>
			<build>
				<plugins>
					<plugin>
						<artifactId>maven-resources-plugin</artifactId>
						<version>2.3</version>
						<executions>
							<execution>
								<id>publish-jboss</id>
								<phase>validate</phase>
								<goals>
									<goal>copy-resources</goal>
								</goals>
								<configuration>
									<encoding>ISO-8859-1</encoding>
									<outputDirectory>${jboss.home}/server/${jboss.config}/deploy/${project.build.finalName}.war</outputDirectory>
									<resources>
										<resource>
											<directory>${basedir}/src/main/webapp</directory>
											<excludes>
												<exclude>**/*.svn</exclude>
												<exclude>**/*.class</exclude>
												<exclude>**/*.jar</exclude>
												<exclude>**/*.MF</exclude>
												<exclude>**/*.xml*</exclude>
											</excludes>
											<filtering>true</filtering>
										</resource>
									</resources>
								</configuration>
							</execution>
						</executions>
					</plugin>
				</plugins>
			</build>
		</profile>
		<profile>
			<id>frontend-test</id>
			<build>
				<plugins>
					<plugin>
						<groupId>com.github.searls</groupId>
						<artifactId>jasmine-maven-plugin</artifactId>
						<version>${jasmine.version}</version>
						<executions>
