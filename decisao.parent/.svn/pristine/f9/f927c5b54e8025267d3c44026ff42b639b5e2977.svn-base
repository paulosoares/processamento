/*Áreas de grupos*/

.group-area  {
 	-webkit-transition: all .3s linear;
    -o-transition: all .3s linear;
    -moz-transition: all .3s linear;
    -ms-transition: all .3s linear;
    -kthtml-transition: all .3s linear;
    transition: all .3s linear;		
}

.group-area .no-itens {
	color: #bbb;
	font-size:20px;

 	-webkit-transition: all .3s linear;
    -o-transition: all .3s linear;
    -moz-transition: all .3s linear;
    -ms-transition: all .3s linear;
    -kthtml-transition: all .3s linear;
    transition: all .3s linear;		
}

.group-area  .group-empty-message {
	color: #bbb;
	padding: 0px 0px 0px 0px;
	font-size:12px;

 	-webkit-transition: all .3s linear;
    -o-transition: all .3s linear;
    -moz-transition: all .3s linear;
    -ms-transition: all .3s linear;
    -kthtml-transition: all .3s linear;
    transition: all .3s linear;			
}

.group-area.grupo-vazio button {
	  /* IE 8 */
	  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)";

	  /* IE 5-7 */
	  filter: alpha(opacity=80);

	  /* Netscape */
	  -moz-opacity: 0.8;

	  /* Safari 1.x */
	  -khtml-opacity: 0.8;

	  /* Good browsers */
	  opacity: 0.8;
}

.group-area.grupo-vazio:hover button {
	  /* IE 8 */
	  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";

	  /* IE 5-7 */
	  filter: alpha(opacity=100);

	  /* Netscape */
	  -moz-opacity: 1;

	  /* Safari 1.x */
	  -khtml-opacity: 1;

	  /* Good browsers */
	  opacity: 1;
}

.group-area.grupo-vazio:hover  {
	border: 5px dashed #888;
}

.group-area.grupo-vazio:hover .no-itens  {
	color: #888;
}

.group-area.grupo-vazio:hover .group-empty-message  {
	color: #888;
}

