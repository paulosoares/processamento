	<modelVersion>4.0.0</modelVersion>
	<artifactId>decisao.plugin</artifactId>
	<name>eSTF-Decis√£o Plugin</name>
	<packaging>jar</packaging>

	<description>Plugin customizado do STFOffice para permitir a edicao de textos no eSTF-Decisao</description>
	<scm>
    <connection>scm:svn:http://build/stf/csol/projetos/java/decisao/src/trunk/decisao.plugin</connection>
    <developerConnection>scm:svn:http://build/stf/csol/projetos/java/decisao/src/trunk/decisao.plugin</developerConnection>
    <url>http://build/stf/csol/projetos/java/decisao/src/trunk/decisao.plugin</url>
  </scm>
	<organization>
	</organization>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-assembly-plugin</artifactId>
				<version>2.2-beta-2</version>
				<executions>
					<execution>
						<id>create-plugin-jar</id>
						<phase>package</phase>
						<goals>
							<goal>single</goal>
						</goals>
						<configuration>
							<descriptors>
								<descriptor>src/main/assembly/jar-with-dependencies-compile.xml</descriptor>
							</descriptors>
							<archive>
								<manifest>
									<mainClass>br.jus.stf.estf.decisao.StfOfficeDecisaoPlugin</mainClass>
								</manifest>
							</archive>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

